$bgColor = lightgray
$colorPrime = #333322
$alpha = 0.8
$amount = 0.85
$fontSize = 16px
$width = 320px
$widthInner = 310px
$fbColor = #3b5998
$red = #B1103C

body
  padding 0
  margin 0
  font-family: "ヒラギノ角ゴ Pro W3", "Hiragino Kaku Gothic Pro", "メイリオ", "Meiryo", verdana, Osaka, "ＭＳ Ｐゴシック", "MS PGothic", Sans-Serif
  width: 100%
  background: $bgColor
  color: $colorPrime
  font-style: normal
  font-variant: normal
  font-weight: normal
  font-size: $fontSize
  overflow: hidden

#content
  width: 640px
  margin: 0 auto
  background: #EEEEEE
  padding: 57px 60px 0 60px
  @media screen and (max-width: 640px)
    width: $width
    padding: 57px 0 0 0

img 
  margin: 0

p 
  margin: 0

a 
  text-decoration: none
  cursor: auto
  color: $fbColor
  font-family: Helvetica, Arial
  font-weight: bold
  &:hover 
    text-decoration: none
    color: #8b9dc3
  
h1 
  display: inline-block
  vertical-align: middle
  text-align: center
  box-sizing: border-box
  font-size: 17px
  font-weight: normal
  color: black
  margin: 0
  padding: 0
  width: 100%
  height: 33px
  line-height: 33px

section 
  margin: 0
  padding: 0

.boxShadow 
  box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.5)

header
  width: 100%
  margin: 0 auto
  z-index: 1

.settings-item
  input[type="range"]
    width: $widthInner*0.7
  h4.distance
    margin: 0
    padding-top: 15px

.card
  width: $widthInner
  height: $widthInner
  background-color: rgba(255,255,255,0.8)
  border: 1px solid gray
  border-radius: 2px
  padding: 5px 0
  img
    height: 100px
    width: 100px
    margin: 0 5px 5px 6px
    border-radius: 3px
    box-shadow: 0px 0px 2px rgba(0, 0, 0, 0.5)
  .profile
    height: 100px
    width: 190px
    h5
      margin: 0
    div.topline
      max-width: 190px
      height: 50px
      overflow: hidden
      span
        height: 50px
        line-height: 50px
    p.bio
      font-size: 12px
      display: -webkit-box
      -webkit-line-clamp: 2
      -webkit-box-orient: vertical
      max-width: 190px
      overflow: hidden
      text-overflow: ellipsis
      height:35px /* fallback */

  li.disabled
    opacity: 0.5

.menu
  span
    text-align: center
    width: 100%

.menu
  width: 60px
  height: 60px
  padding: 10px 0

#menuRight
  float: right

#menuLeft
  float: left

#menuLeft
  form
    display: none

#header 
  margin: 10px 0 10px 0
  width: 200px

header.settings
  #header
    display: none

header.settings
  #menuLeft
    float: right
    width: 260px
    & span
      width: 60px
      float: right
    
    & form
      display: block
      float: right
      padding: 0 0 10px 15px
      & input
        width: 185px
        z-index: 10            


span.glyphicon-th 
  font-size: 2em

span.glyphicon-cog 
  font-size: 2.4em

span.glyphicon-search 
  font-size: 2em


