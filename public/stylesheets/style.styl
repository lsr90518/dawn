  font-size: 30px
  color: grey


.list2 
  margin-bottom: 1px
  background-color: white
  color: grey


.titlehead
  position: fixed
  font-size: 18px
  color: white
  height: 48px
  background-color: $red
  line-height: 48px
  vertical-align: middle
  z-index: 9

.addbutton 
  margin-right: 10px
  font-size: 15px
  color: $red

.container
  margin: 4px
  width: 300px
  height: 40px
  text-align: center
  font-size: 13px

  img
    border-radius: 40px

  .team1
    float: left
  .team2
    float: right

.button-room
  background: white
  border-style: solid
  border-width: 1px
  border-color: gray
  border-radius: 5px
  padding: 10px 5px

.pure-u-1
  background: white
  border-style: solid
  border-width: 1px
  border-color: gray
  border-radius: 5px
  padding: 5px

.pure-g
  img
    border-radius: 50px

#nav
  background: $red
  //border: 1px solid darkgray
  width: 100%
  ul
    margin: 0 auto
    padding: 8px 0
    width: 640px
    @media screen and (max-width: 640px)
      width: 320px
    li
      width: 106px
      text-align: center
      a
        color: white
        font-weight: normal
        font-size: 13px

#layout,
#menu,
.menu-link
  -webkit-transition: all 0.2s ease-out
  -moz-transition: all 0.2s ease-out
  -ms-transition: all 0.2s ease-out
  -o-transition: all 0.2s ease-out
  transition: all 0.2s ease-out

/*
This is the parent `<div>` that contains the menu and the content area.
*/
$sideMenu = 200px

#layout
  position: relative
  padding-left: 0

#layout.active
  position: relative
  left: $sideMenu

#layout.active #menu
  left: $sideMenu
  width: $sideMenu

#layout.active .menu-link
  left: $sideMenu

#menu
  margin-left: -1*$sideMenu
  width: $sideMenu
  position: fixed
  top: 0
  left: 0
  bottom: 0
  z-index: 1000
  background: #191818
  overflow-y: auto
  -webkit-overflow-scrolling: touch

#menu a
  color: $red
  border: none
  padding: 0.6em 0 0.6em 0.6em
  i
    width: 30px
    text-align: center
    font-size: 1.2em
    color: white
    padding-right: 0.6em

#menu .pure-menu,
#menu .pure-menu ul
  border: none
  background: transparent

#menu .pure-menu ul,
#menu .pure-menu .menu-item-divided
  border-top: 1px solid #333

#menu .pure-menu li a:hover,
#menu .pure-menu li a:focus
  background: #333

#menu .pure-menu li.clicked a,
#menu .pure-menu li.clicked a:hover,
#menu .pure-menu li.clicked a:focus
  background: $red
  color: white

#menu .pure-menu-selected,
#menu .pure-menu-heading
  background: $red

#menu .pure-menu-selected a
  color: #fff

#menu .pure-menu-heading
  font-size: 110%
  color: #fff
  margin: 0

.menu-link, .chat-link
  position: fixed
  display: block
  top: 0
  left: 0 
  background: #000
  background: $red
  font-size: 10px
  z-index: 10
  width: 2em
  height: 2em
  padding: 1.4em 1.6em
  i
    color: white  
.chat-link
  left: $width - 52

.menu-link:hover,
.menu-link:focus,
.chat-link:hover,
.chat-link:focus
  background: #000
  
@media (min-width: 48em)
  
  #layout
    padding-left: $sideMenu /* left col width "#menu" */
    left: 0
  
  #menu
    left: $sideMenu
  
  .menu-link
    position: fixed
    left: $sideMenu
    display: none
  .chat-link
    position: fixed
    left: $sideMenu
    display: none
  
  #layout.active .menu-link
    left: $sideMenu

