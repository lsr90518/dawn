ul,
li 
  list-style: none
  padding: 0

ul#matches 
  width: $width - 8
  position: relative
  z-index: 10
  margin-left: 3px

ul#matches
  li
    animation-name: $enlarge
    animation-duration: 1s
    animation-iteration-count: 1
    animation-timing-function: ease-out
    -webkit-animation-name: $enlarge
    -webkit-animation-duration: 1s
    -webkit-animation-iteration-count: 1
    -webkit-animation-timing-function: ease-out   
    display: inline-block
    float: left
    width: $widthInner
    height: $widthInner
    border-radius: 2px
    overflow: hidden
    background-color: white
    margin-bottom: 10px

.sidebyside
  button 
    margin: 0 35px
    border-radius: 60px
    height: 60px
    width: 60px
    padding: 0
    span, i
      display: inline-block
      line-height: 60px
      vertical-align: middle


.pure-button-secondary 
  background-color: rgb(223, 117, 20)
  color: #fff

.passBtn
  font-size: 16px
  span
    margin-top: -3px

.likeBtn
  background: white
  i
    margin-top: 2px
    color: $red

.clearfix:after 
  content: "."
  visibility: hidden
  display: block
  height: 0
  clear: both


.center-wrap 
  text-align: center


.center-wrap:after 
  content: ''
  box-sizing: content-box
  visibility: hidden
  display: inline-block
  height: 0
  vertical-align: middle


.center 
  display: inline-block
  vertical-align: middle


p.center,
a.center 
  text-align: center

.left
  float: left
  vertical-align: bottom

.right
  float: right
  vertical-align: bottom

.list 
